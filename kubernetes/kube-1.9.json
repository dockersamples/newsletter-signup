{
    "apiVersion": "vlabs",
    "properties": {
      "orchestratorProfile": {
        "orchestratorType": "Kubernetes",
        "orchestratorRelease": "1.9"
      },
      "masterProfile": {
        "count": 1,
        "dnsPrefix": "YOUR_DNS_PREFIX",
        "vmSize": "Standard_D2_v2"
      },
      "agentPoolProfiles": [
        {
          "name": "windowspool2",
          "count": 2,
          "vmSize": "Standard_D2_v2",
          "availabilityProfile": "AvailabilitySet",
          "osType": "Windows",
          "storageProfile": "ManagedDisks"
        },        
        {
            "name": "linuxpool2",
            "count": 2,
            "vmSize": "Standard_D2_v2",
            "availabilityProfile": "AvailabilitySet",
            "osType": "Linux",
            "storageProfile": "ManagedDisks"
          }
      ],
      "windowsProfile": {
        "adminUsername": "azureuser",
        "adminPassword": "YOUR_ADMIN_PASSWORD"
      },
      "linuxProfile": {
        "adminUsername": "azureuser",
        "ssh": {
          "publicKeys": [
            {
              "keyData": "YOUR_SSH_PUBLIC_KEY"
            }
          ]
        }
      },
      "servicePrincipalProfile": {
        "clientId": "YOUR_SP_APP_ID",
        "secret": "YOUR_SP_PASSWORD"
      }
    }
  }